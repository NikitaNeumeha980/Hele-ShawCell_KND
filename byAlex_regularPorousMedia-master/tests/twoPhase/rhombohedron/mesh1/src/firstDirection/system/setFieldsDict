FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.phase1 0
);

regions
(
    #include    "droplets"

    zoneToCell
    {
        name film;
        fieldValues
        (
            volScalarFieldValue alpha.phase1 0
        );
    }
);



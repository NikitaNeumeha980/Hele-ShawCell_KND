FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pointSync false;

patches
(
    {
        name cyclicInlet;

        patchInfo
        {
            type patch;
        }

        constructFrom patches;

        patches (inlet);
    }
    {
        name cyclicOutlet;

        patchInfo
        {
            type patch;
        }

        constructFrom patches;

        patches (outlet);
    }
);

